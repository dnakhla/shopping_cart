{
  "extends": "airbnb",
  "plugins": [
    "babel"
  ],
  "rules": {
    "no-console": 0,
    "comma-dangle": 0,
    "max-len": 0,
    "class-methods-use-this": 0,
    "react/require-extension": "off",
    "import/no-extraneous-dependencies": ["error", {"devDependencies": ["**/*.config.js", "**/*.test.js", "**/*.spec.js"]}]
  },
  "env": {
    "browser": true,
    "node": true
  }
}
